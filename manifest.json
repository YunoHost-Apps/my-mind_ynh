    },
    "license": "MIT",
    "maintainer": {
        "name": "Anmol Sharma",
        "email": "anmol@datamol.org",
        "url": "https://datamol.org"
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/mymind",
                "default": "/mymind"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
